#ifndef TTIME_H_
#define TTIME_H_

class TTime {
public:
	TTime();
	TTime(int hour, int min, int sec);
	virtual ~TTime();

	int second;
	int minute;
	int hour;

	int getHour() const;
	void setHour(int hour);
	int getMinute() const;
	void setMinute(int minute);
	void print();
};

#endif /* TTIME_H_ */
