#include "Main.h"

#include <TDate.h>
#include <TTime.h>

Main::Main() {
	TDate t;
	printf("Datum: ");
	t.printDate();

	printf("\n");

	TTime tt;
	printf("Uhrzeit: ");
	tt.printTime();
}

