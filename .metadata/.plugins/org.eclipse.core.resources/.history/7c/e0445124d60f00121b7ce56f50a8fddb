#ifndef TTIME_H_
#define TTIME_H_
#include <string>

class TTime {
public:
	TTime();
	virtual ~TTime();

	int second;
	int minute;
	int hour;

	int getHour() const;
	void setHour(int hour);
	int getMinute() const;
	void setMinute(int minute);
	int getSecond() const;
	void setSecond(int second);
	 void printDate();
};

#endif /* TTIME_H_ */
