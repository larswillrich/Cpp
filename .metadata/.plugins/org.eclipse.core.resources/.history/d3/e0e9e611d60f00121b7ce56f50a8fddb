#include "TTime.h"


TTime::TTime() {

}

TTime::~TTime() {
}

int getHour() const {
	return hour;
}

void setHour(int hour) {
	this->hour = hour;
}

int getMinute() const {
	return minute;
}

void setMinute(int minute) {
	this->minute = minute;
}

int getSecond() const {
	return second;
}

void setSecond(int second) {
	this->second = second;
}
